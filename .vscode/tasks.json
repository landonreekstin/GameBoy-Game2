{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "clean",
            "type": "shell",
            "command": "make",
            "args": ["clean"],
            "options": { "cwd": "${workspaceFolder}/Clean_Up_Crew" }
        },
        {
            "label": "build (Debug)",
            "type": "shell",
            "command": "make",
            "args": ["debug"],
            "options": { "cwd": "${workspaceFolder}/Clean_Up_Crew" },
            "dependsOn": ["clean"],
            "group": { "kind": "build", "isDefault": true }
        },
    ]
}